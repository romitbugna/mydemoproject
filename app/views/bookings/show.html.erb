<div class="container">
<p id="notice"><%= notice %></p>


<div class="col-md-1"></div>
<div class="well col-md-4">
<p>
  <strong>No of seats booked:</strong>
  <%= @booking.no_of_seats_booked %> 

</p>

<p>
  <strong>Seat type:</strong>
  <%= @booking.seat_type %>
</p>

<p>
  <strong>Cost of ticket:</strong>
  <%= find_cost_of_ticket(@booking) %>

</p>

<h4>
<%= link_to 'Print',booking_path(@booking, format: "pdf"), class: "btn btn-success" %> |
<%= link_to 'Book Another', :controller=> 'bookings', :action=> 'new', class: "btn btn-primary" %> </h4>
</div>

</div>